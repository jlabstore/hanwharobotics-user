<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hanwha.robotics.user.mapper.MemberLogMapper">

    <insert id="insertMemberLog" parameterType="com.hanwha.robotics.user.entity.Member">
        INSERT INTO tb_member_log (member_no, access_dt)
        VALUES (#{memberNo}, current_timestamp)
    </insert>

</mapper>